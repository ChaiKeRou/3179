{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": "container",
  "height": 550,
  "params": [
    {
      "name": "zoom_level",
      "value": 2200,
      "bind": {
        "input": "range",
        "min": 2200,
        "max": 6200,
        "step": 1000,
        "name": "Zoom: "
      }
    },
    {
      "name": "center_to",
      "value": [
        109,
        0.5
      ],
      "bind": {
        "input": "select",
        "options": [
          [
            109,
            0.5
          ],
          [
            103,
            4
          ],
          [
            114,
            3.5
          ]
        ],
        "labels": [
          "Malaysia",
          "West Malaysia",
          "East Malaysia"
        ],
        "name": "Map Centre: "
      }
    }
  ],
  "projection": {
    "type": "equirectangular",
    "center": {
      "expr": "center_to"
    },
    "scale": {
      "expr": "zoom_level"
    }
  },
  "background": null,
  "layer": [
    {
      "data": {
        "url": "https://raw.githubusercontent.com/ChaiKeRou/3179/main/js/ocean.json",
        "format": {
          "type": "topojson",
          "feature": "ne_10m_ocean"
        }
      },
      "mark": {
        "type": "geoshape",
        "fill": "#b0e0e6"
      }
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/ChaiKeRou/3179/main/js/ne_10m.json",
        "format": {
          "type": "topojson",
          "feature": "ne_10m_admin_0_countries"
        }
      },
      "mark": {
        "type": "geoshape",
        "stroke": "black",
        "strokeWidth": 0.5,
        "fill": "lightgray"
      }
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/ChaiKeRou/3179/main/js/malaysia.district.json",
        "format": {
          "type": "topojson",
          "feature": "malaysia.district"
        }
      },
      "mark": {
        "type": "geoshape",
        "stroke": "#a98080",
        "strokeWidth": 0.5
      },
      "transform": [
        {
          "lookup": "properties.name",
          "from": {
            "data": {
              "url": "https://raw.githubusercontent.com/ChaiKeRou/3179/main/data/crude_death_state_district.csv"
            },
            "key": "Administrative district",
            "fields": [
              "Crude death rate"
            ]
          }
        }
      ],
      "encoding": {
        "color": {
          "field": "Crude death rate",
          "type": "quantitative",
          "scale": {
            "range": [
              "#fff4ef",
              "#fcae91",
              "#fb6a4a",
              "#a50f15",
              "#760d0f"
            ]
          },
          "legend": {
            "title": "Crude Death Rate (Per 1,000 Population)",
            "orient": "bottom-left",
            "direction": "horizontal",
            "titleFontSize": 11,
            "labelFontSize": 10,
            "gradientLength": 250,
            "titleLimit": 250,
            "offset": 5
          }
        },
        "tooltip": [
          {
            "field": "properties.name",
            "type": "nominal",
            "title": "State"
          },
          {
            "field": "Crude death rate",
            "type": "quantitative",
            "title": "Crude Death Rate"
          }
        ]
      }
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/ChaiKeRou/3179/main/js/malaysia.state.json",
        "format": {
          "type": "topojson",
          "feature": "malaysia.state"
        }
      },
      "mark": {
        "type": "geoshape",
        "stroke": "#4d1f1f",
        "strokeWidth": 1.7,
        "fill": "transparent"
      }
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/ChaiKeRou/3179/main/js/malaysia.district.json",
        "format": {
          "type": "topojson",
          "feature": "malaysia.district"
        }
      },
      "mark": {
        "type": "geoshape",
        "stroke": "#7c5d5d",
        "strokeWidth": 0.5,
        "fill": "transparent"
      },
      "transform": [
        {
          "lookup": "properties.name",
          "from": {
            "data": {
              "url": "https://raw.githubusercontent.com/ChaiKeRou/3179/main/data/crude_death_state_district.csv"
            },
            "key": "Administrative district",
            "fields": [
              "Crude death rate",
              "State"
            ]
          }
        }
      ],
      "encoding": {
        "color": {
          "value": null
        },
        "tooltip": [
          {
            "field": "State",
            "type": "nominal",
            "title": "State"
          },
          {
            "field": "properties.name",
            "type": "nominal",
            "title": "District"
          },
          {
            "field": "Crude death rate",
            "type": "quantitative",
            "title": "Crude Death Rate"
          }
        ]
      }
    }
  ],
  "config": {
    "title": {
      "anchor": "start",
      "offset": 10
    },
    "view": {
      "stroke": null
    }
  }
}