{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "https://raw.githubusercontent.com/ChaiKeRou/3179/main/data/state_diseases.csv"
  },
  "width": "container",
  "background": null,
  "facet": {
    "field": "cause_death",
    "type": "nominal"
  },
  "spec": {
    "width": 380,
    "mark": "bar",
    "encoding": {
      "x": {
        "field": "state",
        "type": "nominal"
      },
      "y": {
        "field": "death_rate",
        "type": "quantitative"
      },
      "color": {
        "field": "gender",
        "type": "nominal",
        "legend": null,
        "scale": {
          "range": [
            "#e30133",
            "#2f88a6"
          ]
        }
      }
    }
  },
  "config": {
    "facet": {
      "columns": 2
    }
  },
  "resolve": {
    "axis": {
      "x": "independent"
    }
  }
}